{
  "name": "base-cypress-automation",
  "version": "1.0.0",
  "description": "The base automation with cypress",
  "author": "Siva Reddy",
  "license": "ISC",
  "scripts": {
    "prettier:check": "prettier --check cypress/**/*.ts",
    "lint:staged": "lint-staged",
    "clean:reports": "rm -R -f cypress/reports && mkdir -p cypress/reports && mkdir -p cypress/reports/mochareports && mkdir -p cypress/reports/mocha",
    "clean:results": "rm -R -f cypress/results && mkdir -p cypress/results && mkdir -p cypress/results/screenshots && mkdir -p cypress/results/detailsCommandLogs",
    "combine-reports": "mochawesome-merge \"cypress/results/*.json\" > cypress/reports/mochawesome-merged.json",
    "generate-reports": "rm -f cypress/reports/mochareports/report.html && marge cypress/reports/mochawesome-merged.json --reportDir cypress/reports/mochareports --reportFilename report --inlineAssets"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "cypress": "^13.15.2",
    "cypress-terminal-report": "^7.0.4",
    "cypress-wait-until": "^1.7.2",
    "dayjs": "^1.10.4",
    "deep-keys": "^0.5.0",
    "flat": "^6.0.1",
    "he": "^1.1.1",
    "inversify": "^6.0.1",
    "mailosaur": "^8.7.0",
    "pg": "^8.8.0",
    "recursive-diff": "^1.0.9",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@cypress/grep": "^4.1.0",
    "@eslint/js": "^9.15.0",
    "@types/chai-json-schema": "1.4.5",
    "@types/eslint": "^8.40.0",
    "@types/lodash": "^4.14.21",
    "@types/node": "^22.10.7",
    "@types/uuid": "^8.3.4",
    "cypress-mochawesome-reporter": "3.2.2",
    "cypress-multi-reporters": "^1.6.1",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-chai-friendly": "0.6.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.3.0",
    "globals": "^15.12.0",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "node-fetch": "2.6.9",
    "prettier": "^3.5.3",
    "pretty-quick": "^2.0.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  },
  "packageManager": "yarn@4.12.0",
  "workspaces": [
    "packages/*"
  ]
}
